[models]
default_image_model = runwayml/stable-diffusion-v1-5
default_video_model = guoyww/animatediff-motion-adapter-v1-5-2
model_cache_dir = ./models_cache
max_memory_usage = 0.8

[generation]
default_steps = 20
default_guidance_scale = 7.5
default_width = 512
default_height = 512
default_video_frames = 16
default_video_fps = 8

[performance]
use_xformers = true
use_tensorrt = false
enable_cpu_offload = true
use_attention_slicing = true
enable_model_cpu_offload = false

[ui]
theme = dark
show_advanced_options = false
auto_save_outputs = true
output_directory = ./outputs

[cloud]
enable_cloud_gpu = false
preferred_provider = aws
max_cloud_cost_per_hour = 5.0

[logging]
log_level = INFO
log_file = ./logs/app.log
enable_performance_metrics = true

[security]
allow_remote_access = false
require_api_key = false
max_requests_per_minute = 60